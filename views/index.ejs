<%- include("./partials/header")%>
    <!-- student form section -->
    <div class="container-fluid">
        <div class="row">
            <!-- student list section -->
            <div class="col-12">
                <div class="header">
                    <div>
                        <h3 class="heading">Dental Appointment Record</h3>
                    </div>
                    <div class="header-action">
                        <div class="excel-section">
                            <form action="/jsontoexcel" method="get">
                                <!-- excel export btn -->
                                <button type="submit" title="export excel" class="export-btn">
                                    <span class="export-icon">
                                        <i class="fa-solid fa-file-excel"></i>
                                    </span>
                                    <span class="title-text">Export excel</span>
                                </button>
                            </form>
                        </div>
                    </div>

                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover">
                        <tr>
                            <th>No</th>
                            <th>Name</th>
                            <th>Diagnostic</th>
                            <th>Doctor</th>
                            <th>Date</th>
                            <th>Time</th>
                            <th>Contact</th>
                            <th>Email</th>
                        </tr>
                        <% text.forEach((value)=>{ %>
                            <tr>
                                <td>
                                    <%= index++ %>
                                </td>
                                <td>
                                    <%= value.name %>
                                </td>
                                <td>
                                    <%= value.diagnostic %>
                                </td>
                                <td>&nbsp;Dr <%= value.doctor %>
                                </td>
                                <td>
                                    <%= value.date %>
                                </td>
                                <td>
                                    <%= value.time %>
                                </td>
                                <td>
                                    <%= value.contact %>
                                </td>
                                <td>
                                    <%= value.email %>
                                </td>

                            </tr>
                            <% }) %>
                    </table>
                </div>

            </div>
        </div>
    </div>
    <%- include("./partials/footer")%>